{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"tournamentSyncEnabled": {
			"type": "boolean",
			"default": false
		},
		"tournamentSlug": {
			"type": "string",
			"default": ""
		},
		"availableQueues": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object",
				"properties": {
					"streamName": {
						"type": "string",
						"default": ""
					},
					"activeSet": {
						"type": "object",
						"properties": {
							"id": {
								"type": "number",
								"default": -1
							},
							"round": {
								"type": "number",
								"default": -1
							},
							"lPlacement": {
								"type": "number",
								"default": -1
							},
							"startedAt": {
								"type": "number",
								"default": -1
							}
						}
					}
				}
			}
		},
		"availableEvents": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object",
				"properties": {
					"id": {
						"type": "number",
						"default": -1
					},
					"name": {
						"type": "string",
						"default": ""
					},
					"type": {
						"type": "number",
						"default": -1
					}
				}
			}
		},
		"selectedQueue": {
			"type": "number",
			"default": -1
		},
		"selectedEvent": {
			"type": "number",
			"default": -1
		},
		"boAutomationEnabled": {
			"type": "boolean",
			"default": false
		},
		"boIntegerThreshold": {
			"type": "number",
			"default": -1
		}
	}
}
